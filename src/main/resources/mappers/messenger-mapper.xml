<mapper namespace="messengerMapper">
	
	
	<insert id="insertEm" parameterType="Emergency">
		INSERT INTO EMERGENCY_LOG
		VALUES(SEQ_EID.NEXTVAL, #{location}, #{writer},#{code})
	</insert>
	
	
</mapper>